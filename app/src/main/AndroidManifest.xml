<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.musicmg">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.MusicMG">
        <activity
            android:name=".Activities.MyHandler"
            android:exported="false" />
        <activity
            android:name=".Activities.ListenSongs"
            android:exported="false" />
        <activity
            android:name=".Activities.AdminCategory"
            android:exported="false" />
        <activity
            android:name=".Activities.InsertSongCategory"
            android:exported="false" />
        <activity
            android:name=".Activities.InsertSong"
            android:exported="false" />
        <activity
            android:name=".Activities.Admin"
            android:exported="false" />
        <activity
            android:name=".Activities.Test"
            android:exported="false" />
        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>